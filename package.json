{
  "name": "cli-starter",
  "version": "0.5.0",
  "main": "cli.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/servexyz/cli-starter"
  },
  "bin": {
    "cli-starter": "./build/cli.js"
  },
  "files": ["./build/cli.js"],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "ava": "^1.0.0-beta.1",
    "chmod": "^0.2.1"
  },
  "scripts": {
    "start": "npm run build && npm run simple-start",
